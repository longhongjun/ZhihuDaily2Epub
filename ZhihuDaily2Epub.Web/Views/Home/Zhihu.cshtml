@{
    ViewBag.Title = "知乎日报下载";
}

<h2>知乎日报下载</h2>
<p>每天20:30生成当日文件</p>
@{string[]
    items = (string[]) ViewBag.Files;
}
<table>
    @foreach (var item in items)
    {
        <tr>
            <td><a href="/epub/@Path.GetFileName(item)">@Path.GetFileName(item)</a></td>
        </tr>
    }
</table>
    